	}

	public String getUserName() {
		return UserName;
	}

	public void setUserName(String userName) {
		UserName = userName;
	}

	public Bitmap getProfileImg() {
		if(ProfileImg==null){
			Bitmap defaultBitmap = ;
		}
		return ProfileImg;
	}

	public void setProfileImg(Bitmap profileImg) {
		ProfileImg = profileImg;
	}
	
	
	
}
